  "license": "MIT",
  "private": true,
  "scripts": {
    "build-prod": "NODE_ENV=production && webpack --config webpack.prod.config.ts",
    "build-dev": "NODE_ENV=development && webpack --config webpack.dev.config.ts",
    "watch-dev": "NODE_ENV=development && webpack --config webpack.dev.config.ts --watch",
    "eslint": "eslint --cache src/**/*.ts layers/**/*.ts",
    "fn": "scripts/friendly-sam.sh fn $1",
    "db": "scripts/friendly-sam.sh db $1",
    "validate": "sam validate --template template.yml",
    "clean": "rm -rf .aws-sam",
    "deploy-prod": "sam deploy --template-file ./.aws-sam/build/template.yaml --stack-name sfw --s3-bucket sfw-deploy-ufddzmpkg --capabilities CAPABILITY_NAMED_IAM"
  },
